<!DOCTYPE html>
<!--[if lt IE 7]><html lang="ru" class="lt-ie9 lt-ie8 lt-ie7"><![endif]-->
<!--[if IE 7]><html lang="ru" class="lt-ie9 lt-ie8"><![endif]-->
<!--[if IE 8]><html lang="ru" class="lt-ie9"><![endif]-->
<!--[if gt IE 8]><!-->
<html lang="ru">
<!--<![endif]-->
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - Multitronics</title>
    <meta name="SITE" content="Authors:Vadim Gorkun & Olexandr Panov" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="shortcut icon" href="favicon.png" />
    @*@Styles.Render("~/bundles/styles")*@
    <link rel="stylesheet" href="/Content/libs/bootstrap/bootstrap-grid-3.3.1.css" />
    <link rel="stylesheet" href="/Content/libs/font-awesome-4.7.0/css/font-awesome.min.css" />
    <link rel="stylesheet" href="/Content/libs/fancybox/jquery.fancybox.css" />
    <link rel="stylesheet" href="/Content/libs/owl-carousel/owl.carousel.css" />
    <link rel="stylesheet" href="/Content/libs/countdown/jquery.countdown.css" />
    <link rel="stylesheet" href="/Content/fonts.css" />
    <link rel="stylesheet" href="/Content/main.css" />
    <link rel="stylesheet" href="/Content/media.css" />
    @RenderSection("styles", required: false)
</head>
<body>
    <div class="hidden">
        <form id="callback" class="call_form">
            <h3>Заказать обратный звонок</h3>
            <input type="text" name="name" placeholder="Ваше имя..." required />
            <input type="text" name="phone" placeholder="Ваш телефон..." required />
            <button class="sell_button" type="submit">Заказать</button>
        </form>
    </div>

    <header class="top_header">
        <div class="header_topline">
            <div class="container">
                <div class="col-md-12">
                    <div class="row">
                        <button class="auth_button hidden-md hidden-lg hidden-sm"><i class="fa fa-sign-in" aria-hidden="true"></i></button>
                        <div class="top_links">

                            <a href="ent.html">Entrance</a> /
                            <a href="reg.html">Check in</a>

                        </div>

                        <div class="soc_buttons">
                            <a href="#"><i class="fa fa-user-circle soc_icons" aria-hidden="true"></i></a>
                            <a href="#"><i class="fa fa-shopping-basket soc_icons" aria-hidden="true"></i></a>
                            <a href="#"><i class="fa fa-envelope soc_icons" aria-hidden="true"></i></a>
                        </div>

                    </div>
                </div>
            </div>
        </div>
        <div class="container">
            <div class="col-md-12">
                <div class="row">
                    <div class="logo_conacts">
                        <div class="logo">
                            <a href="/" class="logo">LOGO</a>
                        </div>
                        <div class="top_contacts">
                            <i class="fa fa-mobile" aria-hidden="true"></i> +38(XXX)XXX-XX-XX<br>
                            <i class="fa fa-mobile" aria-hidden="true"></i> +38(XXX)XXX-XX-XX<br>
                            <i class="fa fa-mobile" aria-hidden="true"></i> +38(XXX)XXX-XX-XX
                        </div>
                    </div>
                </div>
                <div class="row">
                    <nav class="main_menu clearfix">
                        <button class="main_menu_button hidden-md hidden-lg"><i class="fa fa-bars" aria-hidden="true"></i></button>
                        <ul>
                            <li>@Html.ActionLink("Товары", "Products", "Product")</li>
                            <li><a href="#">Услуги</a></li>
                            <li><a href="#">Наши работы</a></li>
                            <li><a href="#">Отзывы</a></li>
                            <li><a href="#">Как купить</a></li>
                            <li><a href="#">Контакты</a></li>
                        </ul>
                        <div class="search">

                            <form class="form-wrapper cf">
                                <input type="text" placeholder="Search here..." required>
                                <button type="submit">Search</button>
                            </form>

                        </div>
                    </nav>
                    <h1>some text</h1>

                </div>
            </div>
        </div>
    </header>
    <section class="content">

        <div class="container">

            <div class="row">

                <div class="col-md-7">
                    <div class="sell_left">Просто отправте заявку, и мы Вам перезвоним!</div>
                </div>
                <div class="col-md-5">
                    <div class="sell_right">
                        <a href="#callback" class="sell_button fancybox">Заказать обратный звонок</a>
                    </div>
                </div>

            </div>
        </div>
    </section>

    <section class="content2">
        <div class="container">
            @RenderBody()
        </div>
    </section>

    <footer class="footer">
        <div class="container">
            <div class="footer-entry">
                Copyright &copy; 2017
            </div>
            <div id="scrollup"><img alt="Прокрутить вверх" src="img/icon_scroll.png"></div>
        </div>
    </footer>
    <!--[if lt IE 9]>
    <script src="/Content/libs/html5shiv/es5-shim.min.js"></script>
    <script src="/Content/libs/html5shiv/html5shiv.min.js"></script>
    <script src="/Content/libs/html5shiv/html5shiv-printshiv.min.js"></script>
    <script src="/Content/libs/respond/respond.min.js"></script>
    <![endif]-->
    @Scripts.Render("~/bundles/scripts")
    <!-- Yandex.Metrika counter --><!-- /Yandex.Metrika counter -->
    <!-- Google Analytics counter --><!-- /Google Analytics counter -->
    @RenderSection("scripts", required: false)

</body>
</html>



